{
  "name": "bismartcliente",
  "productName": "BiSmartClient",
  "version": "1.0.19",
  "description": "BISMART vers√£o para clientes",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.smart.bismartclient",
    "asar": true,
    "files": [
      "main.js",
      "preload.js",
      "scripts/**/*",
      "pages/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json",
      "!dist/**",
      "!**/*.map",
      "!**/*.md",
      "!out/**"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "caio-henrique2006",
        "repo": "BiSmartClient"
      }
    ],
    "compression": "maximum"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caio-henrique2006/BiSmartClient.git"
  },
  "keywords": [
    "bismart",
    "background-process"
  ],
  "author": "Caio Henrique",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caio-henrique2006/BiSmartClient/issues"
  },
  "homepage": "https://github.com/caio-henrique2006/BiSmartClient#readme",
  "devDependencies": {
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^33.2.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.12.0",
    "update-electron-app": "^3.1.1"
  }
}
